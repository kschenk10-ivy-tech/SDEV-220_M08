{
    "files.autoSave": "onFocusChange",
    "editor.renderWhitespace": "none",
    "ipynb.experimental.serialization": false,
    "ipynb.pasteImagesAsAttachments.enabled": false,
    "github.branchProtection": false,
    "github.gitAuthentication": false,
    "github.showAvatar": false,
    "scm.autoReveal": false,
    "scm.compactFolders": false,
    "scm.countBadge": "off",
    "scm.diffDecorations": "none",
    "scm.diffDecorationsGutterAction": "none",
    "scm.diffDecorationsGutterPattern": {
        "modified": false
    },
    "scm.diffDecorationsGutterWidth": 1,
    "scm.diffDecorationsIgnoreTrimWhitespace": "true",
    "scm.graph.badges": "all",
    "scm.graph.pageOnScroll": true,
    "scm.repositories.sortOrder": "path",
    "scm.showActionButton": false,
    "scm.showInputActionButton": false,
    "editor.wordWrap": "on",
    "diffEditor.experimental.showEmptyDecorations": false,
    "diffEditor.diffAlgorithm": "legacy",
    "diffEditor.renderGutterMenu": false,
    "editor.renderLineHighlight": "none",
    "editor.showFoldingControls": "never",
    "editor.guides.indentation": false,
    "editor.glyphMargin": false,
    "editor.colorDecorators": false,
    "editor.minimap.renderCharacters": false,
    "diffEditor.renderIndicators": false,
    "diffEditor.renderMarginRevertIcon": false,
    "diffEditor.renderSideBySide": false,
    "editor.renderControlCharacters": false,
    "editor.renderFinalNewline": "off",
    "editor.experimentalWhitespaceRendering": "off",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.foldingHighlight": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.highlightActiveIndentation": false,
    "editor.occurrencesHighlight": "off",
    "editor.selectionHighlight": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.includeStrings": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.matchBrackets": "never"
}